{
  "name": "@replrn/parser-utility",
  "version": "0.0.0",
  "inherits": "<root>/.templates/ts-library/package.json",
  "template": "ts-library",
  "main": "src/index.ts",
  "type": "module",
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "dist/index.d.ts",
        "import": "dist/index.js",
        "require": "dist/index.umd.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "mdast-util-from-markdown": "^2.0.2",
    "unist-util-visit": "^5.0.0"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run"
  }
}